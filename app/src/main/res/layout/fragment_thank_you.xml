<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_thanku"
    android:orientation="vertical"
    tools:context="sa.upscale.coworking.MettingRoom.ThankYou">

    <!--<include layout="@layout/toolbar_layout" />-->

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/img_thankyou_close"
                android:layout_margin="@dimen/margin10"
                android:layout_gravity="right"
                android:src="@drawable/close_1"
                android:layout_width="20dp"
                android:layout_height="20dp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="150dp"
                    android:layout_height="200dp"
                    android:layout_gravity="center"
                    android:src="@drawable/logo1" />

                <sa.upscale.coworking.CustomFont.MytextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/margin10"
                    android:gravity="center"
                    android:text="@string/thank_you"
                    android:textColor="@color/white"
                    android:textSize="30dp" />

                <sa.upscale.coworking.CustomFont.MytextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:gravity="center"
                    android:text="@string/for_booking_with_us"
                    android:textColor="@color/spaceItem"
                    android:textSize="15dp" />

                <sa.upscale.coworking.CustomFont.MytextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:gravity="center"
                    android:text="@string/if_you_want_to_contact_us_please_reach_us"
                    android:textColor="@color/spaceItem"
                    android:textSize="15dp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    android:gravity="center">

                    <sa.upscale.coworking.CustomFont.MytextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:text="@string/on"
                        android:textColor="@color/spaceItem"
                        android:textSize="15dp" />

                    <sa.upscale.coworking.CustomFont.MytextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginLeft="@dimen/margin5"
                        android:text="@string/booking_upscale_com_sa"
                        android:textColor="@color/cardview_dark_background"
                        android:textSize="13dp" />

                </LinearLayout>

                <sa.upscale.coworking.CustomFont.MytextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="@dimen/margin5"
                    android:layout_marginTop="@dimen/margin5"
                    android:gravity="center"
                    android:text="@string/booking_ref_number"
                    android:textColor="@color/cardview_dark_background"
                    android:textSize="15dp" />

                <LinearLayout
                    android:layout_width="250dp"
                    android:layout_height="50dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/margin20"
                    android:background="@drawable/list_ur_space">

                    <sa.upscale.coworking.CustomFont.MytextView
                        android:id="@+id/tv_tq_refranceCode"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text="1235555"
                        android:textColor="@color/white"
                        android:textSize="15dp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="250dp"
                    android:layout_height="50dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/margin10"
                    android:background="@drawable/not_register">

                    <sa.upscale.coworking.CustomFont.MytextView
                        android:id="@+id/tv_tq_invitation"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text="INVITATIONS"
                        android:textColor="@color/white"
                        android:textSize="15dp" />

                </LinearLayout>

                <sa.upscale.coworking.CustomFont.MytextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="@dimen/margin5"
                    android:layout_marginTop="@dimen/margin20"
                    android:gravity="center"
                    android:text="@string/sharing_with_social_media"
                    android:textColor="@color/cardview_dark_background"
                    android:textSize="15dp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginBottom="30dp"
                    android:layout_marginTop="@dimen/margin20"
                    android:gravity="center"
                    android:orientation="horizontal">


                    <ImageView
                        android:id="@+id/img_thankyou_facebook"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_marginLeft="@dimen/margin10"
                        android:layout_marginRight="@dimen/margin10"
                        android:src="@drawable/fb_tq" />

                    <ImageView
                        android:id="@+id/img_thankyou_twitter"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_marginLeft="@dimen/margin10"
                        android:layout_marginRight="@dimen/margin10"
                        android:src="@drawable/tw_tq" />


                    <ImageView
                        android:id="@+id/img_thankyou_googlePlus"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_marginLeft="@dimen/margin10"
                        android:layout_marginRight="@dimen/margin10"
                        android:src="@drawable/google_tq" />

                </LinearLayout>

            </LinearLayout>
        </FrameLayout>
    </ScrollView>


</LinearLayout>
